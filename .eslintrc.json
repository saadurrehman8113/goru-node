{
  "env": {
    "es2023": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:import/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/no-unresolved": "off"
  },
  "ignorePatterns": ["node_modules/", "dist/"]
}
